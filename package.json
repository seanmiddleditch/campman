{
  "name": "campman",
  "version": "0.1.1",
  "description": "Campaign management software for RPGs",
  "main": "app/dist/server.js",
  "scripts": {
    "start": "node app/dist/app/server.js",
    "serve": "nf start -x 8080",
    "clean": "rimraf \"app/dist/*\" \"client/dist/*\"",
    "build": "tsc -p app/tsconfig.json",
    "build-dev": "tsc --watch -p app/tsconfig.json",
    "webpack": "webpack-cli --config client/webpack.config.js",
    "webpack-dev": "webpack-cli --watch --config client/webpack.config.js",
    "preinstall": "npm run clean",
    "postinstall": "npm run build && npm run webpack",
    "migrations": "typeorm migrations:run"
  },
  "engines": {
    "node": "~8.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seanmiddleditch/campman"
  },
